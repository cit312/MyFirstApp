<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfirstapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.myfirstapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- created by Brayden at 2013-07-03 16:56:21 -->

        <activity
            android:name="com.example.myfirstapp.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.myfirstapp.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.myfirstapp.MainActivity" />
        </activity>
        <activity
            android:name="com.example.myfirstapp.PivotInfo"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.myfirstapp.PivotInfo" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.myfirstapp.PivotInfo" />
        </activity>
        <activity
            android:name="com.example.myfirstapp.NewPivotActivity"
            android:label="@string/title_activity_new_pivot" >
        </activity>
        <activity
            android:name="com.example.myfirstapp.NewLogActivity"
            android:label="@string/title_activity_new_log" >
        </activity>
        <activity
            android:name="com.example.myfirstapp.WeatherActivity"
            android:label="@string/title_activity_weather" >
        </activity>
    </application>

</manifest>
